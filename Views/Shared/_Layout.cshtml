<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Frisk_2._0</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Frisk_2._0.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Frisk_2._0.Member.css"/>
</head>
<body>
    <header>
    <nav class="navbar">

        <ul class="nav-list">
            <li class="nav-item">
                <a class="navbar-brand " asp-area="" asp-controller="Home" asp-action="Index">Frisk</a>
            </li>
            <li class="nav-item">
                <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Index">Hem</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Event</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Om oss</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Login" asp-action="Index">Logga in</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="SignUp" asp-action="Index">Bli medlem</a>
            </li>
        </ul>
    </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Frisk_2._0 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
