@{
    ViewData["Title"] = "Home Page";
}
<link href="~/css/landingpage.css" rel="stylesheet" />

    @if (Model != null)
    {
        <h1>Välkommen, @Model.FirstName!</h1>
        <p>Din e-postadress: @Model.Email</p>
        <p>Ditt efternamn: @Model.LastName</p>
    }
    else
    {
        
    }
    <hero class="hero-image">
    
    <div class="hero-container">
                <p class="heroHeading">Hitta event nära dig!</p>
    
        <div class="input-cont">
        <div class="search-cont">
        <form>
            <input class="input-style" type="text" placeholder="Sök event här..." />
        </form>
        </div>
    </div>
     <div>       
       
       <button class="button-style">Sök</button>
       
    </div>
    </div>

</hero>

