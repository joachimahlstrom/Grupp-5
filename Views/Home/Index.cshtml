@model Frisk_2._0.Models.UserData

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @if (Model != null)
    {
        <h1>Välkommen, @Model.FirstName!</h1>
        <p>Din e-postadress: @Model.Email</p>
        <p>Ditt efternamn: @Model.LastName</p>
    }
    else
    {
        <h1>Välkommen till Frisk 2.0</h1>
    }
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
